model CardItem {
    id        String   @id @default(uuid())
    userId String
    mealId String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    user User @relation(fields: [userId], references: [id])
    meal Meal @relation(fields: [mealId], references: [id])

    @@index([mealId])
    @@index([userId])

    @@map("CardItems")
}
